{
	"name": "babel-improved-gulp-pipelines",
	"description": "transpiles gulp to pipes in specific gulp case",
	"author": "Edwin Kofler",
	"license": "MIT",
	"keywords": [
		"babel",
		"plugin",
		"babel-plugin",
		"gulp",
		"pipeline"
	],
	"main": "index.js",
	"scripts": {
		"format": "prettier --write '**/*'",
		"test": "ava '**/*.test.js'"
	},
	"ava": {
		"require": [
			"esm"
		]
	},
	"devDependencies": {
		"@babel/core": "^7.9.0",
		"ava": "^3.5.1",
		"esm": "^3.2.25",
		"prettier": "^2.0.2"
	},
	"peerDependencies": {
		"@babel/core": "^7.0.0-0"
	}
}
